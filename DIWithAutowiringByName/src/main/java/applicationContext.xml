<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="address1" class="com.javatpoint.Address">
		<property name="city" value="Lahore"></property>
		<property name="state" value="Punjab"></property>
		<property name="country" value="Pakistan"></property>
	</bean>
	<bean id="user" class="com.javatpoint.User" autowire="constructor">
	</bean>
</beans>  
