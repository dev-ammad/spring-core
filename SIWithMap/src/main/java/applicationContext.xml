<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="question" class="com.javatpoint.Question">
		<property name="id" value="1"></property>
		<property name="name" value="Who is PM of Pakistan?"></property>
		<property name="answers">
			<map>
				<entry key="Imran Khan" value="Hammad"></entry>
				<entry key="Nawaz Sfarif" value="Ammad"></entry>
				<entry key="Arif ALvi" value="Usman"></entry>
			</map>
		</property>
	</bean>
</beans>  
